<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Silver-Gray Palette-->

    <Color x:Key="LightGray">#E4ECF2</Color>
    <SolidColorBrush x:Key="LightGraySolid"         Color="{StaticResource LightGray}"/>

    <Color x:Key="DarkGray">#5B7F9A</Color>
    <SolidColorBrush x:Key="DarkGraySolid"          Color="{StaticResource DarkGray}"/>

    <Color x:Key="HoverGray">#B9CCDA</Color>
    <SolidColorBrush x:Key="HoverGraySolid"         Color="{StaticResource HoverGray}"/>

    <Color x:Key="SeperatorGray">#A9BCCA</Color>
    <SolidColorBrush x:Key="SeperatorColorSolid"    Color="{StaticResource SeperatorGray}"/>

    <Color x:Key="DarkGreen">#4C884D</Color>
    <SolidColorBrush x:Key="DarkGreenSolid"         Color="{StaticResource DarkGreen}"/>

    <Color x:Key="DarkRed">#651919</Color>
    <SolidColorBrush x:Key="DarkRedSolid"           Color="{StaticResource DarkRed}"/>

    <Color x:Key="TextGray">#3C3C3C</Color>
    <SolidColorBrush x:Key="TextGraySolid"          Color="{StaticResource TextGray}"/>

    <!--#endregion-->

</ResourceDictionary>